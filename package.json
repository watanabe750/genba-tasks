    "diagram:infra": "mmdc -i docs/screens/readme-assets/infra.mmd -o docs/screens/readme-assets/infra.png",
    "diagram:er": "mmdc -i backend/docs/screens/readme-assets/er.mmd -o backend/docs/screens/readme-assets/er.png",
    "diagram:all": "pnpm run diagram:infra && pnpm run diagram:er"
  },
  "dependencies": {
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@playwright/test": "^1.46.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/react-router-dom": "^5.3.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef"
